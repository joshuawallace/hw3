#This is the Makefile

CFLAGS = -Wall -std=c99
#CC = gcc
LIB = -lm -g


duffing_euler: euler.o duffing.o
	$(CC) $(CFLAGS) -o duffing_euler duffing.c euler.c $(LIB)

duffing_ab2: ab2.o duffing.o
	$(CC) $(CFLAGS) -o duffing_ab2 duffing.c ab2.c $(LIB)

duffing_rk4: rk4.o duffing.o
	$(CC) $(CFLAGS) -o duffing_rk4 duffing.c rk4.c $(LIB)

lanemden_rk4: rk4.o lanemden.o
	$(CC) $(CFLAGS) -o lanemden_rk4 lanemden.c rk4.c $(LIB)

lanemden_ab2: ab2.o lanemden.o
	$(CC) $(CFLAGS) -o lanemden_ab2 lanemden.c ab2.c $(LIB)

lanemden.hw5_rk4: rk4.o lanemden.hw5.o
	$(CC) $(CFLAGS) -o lanemden.hw5_rk4 lanemden.hw5.c rk4.c $(LIB)